    "rss-parser": "^3.13.0",
    "node-cron": "^3.0.3",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "@sendgrid/mail": "^8.1.0",
    "twilio": "^4.19.0",
    "apn": "^2.2.0",
    "joi": "^17.11.0",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "winston": "^3.11.0",
    "bull": "^4.11.5",
    "redis": "^4.6.11"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
