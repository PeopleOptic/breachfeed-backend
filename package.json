{
  "name": "breachfeed-backend",
  "version": "1.0.0",
  "description": "Backend API for BreachFeed cybersecurity notification system",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "start:prod": "prisma migrate deploy && node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "build": "prisma generate",
    "db:migrate": "prisma migrate dev",
